from typing import List
def even_list(int_list: List[int]) -> List[int]:
"""
Determines if a number is even and return an even list.
Args:
int_list: A list of integer.
Returns:
A list of even integers.
[:] == 복사본 사용, 원본 냅둬서 remove안정적
"""
    for x in int_list[:]:
        if (x % 2 != 0):
            int_list.remove(x)
        return int_list

